<VBox fx:id="profilePage" prefHeight="250.0" prefWidth="300.0"
      style="-fx-max-height: 250; -fx-max-width: 300; -fx-background-color: #000; -fx-background-radius: 15;"
      xmlns:fx="http://javafx.com/fxml" fx:controller="program.chatus.ProfilePageController">

   <children>
      <HBox fx:id="UserDetailsProfile" alignment="CENTER_LEFT" prefHeight="94.0" prefWidth="300.0"
            style="-fx-background-color: #131313; -fx-background-radius: 15 15 0 0;">
         <children>
            <ImageView fitHeight="58.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/user (1).png" />
               </image>
            </ImageView>
            <VBox alignment="CENTER_LEFT" prefHeight="98.0" prefWidth="200.0">
               <children>
                  <Label text="Actions" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <Label text="Choose an option" textFill="#4fff23">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets left="10.0" right="30.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>

      <VBox alignment="CENTER" spacing="10" style="-fx-background-color: #000;">
         <children>
            <Button text="Delete" onAction="#onDelete"
                    style="-fx-background-color: #ff4c4c; -fx-text-fill: white; -fx-background-radius: 8;" prefWidth="200" />
            <Button text="Block" onAction="#onBlock"
                    style="-fx-background-color: #ff8c00; -fx-text-fill: white; -fx-background-radius: 8;" prefWidth="200" />
            <Button text="Report" onAction="#onReport"
                    style="-fx-background-color: #ffcc00; -fx-text-fill: black; -fx-background-radius: 8;" prefWidth="200" />
         </children>
         <VBox.margin>
            <Insets top="20.0" bottom="20.0" left="50.0" right="50.0"/>
         </VBox.margin>
      </VBox>
   </children>

   <effect>
      <DropShadow>
         <color>
            <Color red="0.21176470816135406" green="0.21176470816135406" blue="0.21176470816135406" />
         </color>
      </DropShadow>
   </effect>
</VBox>

